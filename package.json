{
  "name": "ansible",
  "version": "0.1.0",
  "description": "Distributed coordination layer for OpenClaw - one agent, multiple bodies",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "openclaw.plugin.json",
    "README.md"
  ],
  "openclaw": {
    "extensions": ["./dist/index.js"]
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "openclaw",
    "plugin",
    "distributed",
    "coordination",
    "yjs",
    "crdt"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "yjs": "^13.6.0",
    "y-websocket": "^2.0.0",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "openclaw": "*"
  },
  "engines": {
    "node": ">=22.22.0"
  }
}
